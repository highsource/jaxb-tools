	</element>
	<element name="one-to-one" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="key-many-to-one" type="class">
		<attribute name="class" property="Clazz"/>
		<attribute name="column" property="ColumnName"/>
	</element>
	<element name="meta-value" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="component" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="one-to-many" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="many-to-many" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="composite-element" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="nested-composite-element" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="map-key-many-to-many" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="index-many-to-many" type="class">
		<attribute name="class" property="Clazz"/>
		<attribute name="column" property="ColumnName"/>
	</element>
	<element name="composite-map-key" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="composite-index" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="generator" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="return" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="id" type="class">
		<attribute name="column" property="ColumnName"/>
