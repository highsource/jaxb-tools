		<attribute name="subselect" property="SubselectAttribute"/>
	</element>
	<element name="join" type="class">
		<attribute name="subselect" property="SubselectAttribute"/>
	</element>
	<element name="union-subclass" type="class">
		<attribute name="subselect" property="SubselectAttribute"/>
	</element>
	<element name="property" type="class">
		<attribute name="type" property="TypeAttribute"/>
	</element>
