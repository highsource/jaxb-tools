            <xsd:element name="memorySize" type="xsd:integer" minOccurs="0"/>                
            <xsd:element name="zfsUsed" type="xsd:boolean" minOccurs="0"/>
            <xsd:element name="svmUsed" type="xsd:boolean" minOccurs="0"/>
            <xsd:element name="vxvmUsed" type="xsd:boolean" minOccurs="0"/>
            <xsd:element name="sdcErrors" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>                
            
            <!-- complex elements -->
                
            <xsd:element name="os" type="ns:osType"/>
            <xsd:element name="systemBoard" type="ns:systemBoardType" minOccurs="0" maxOccurs="unbounded"/>
            
            <xsd:element name="cpu" type="ns:cpuType" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="dimm" type="ns:dimmType" minOccurs="0" maxOccurs="unbounded"/>          
            <xsd:element name="disk" type="ns:diskType" minOccurs="0" maxOccurs="unbounded"/>
