         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DepartmentName" type="NameType" />
   <xsd:element name="FamilyName" type="NameType">
      <xsd:annotation>
         <xsd:documentation source="http://www.openapplications.org/oagis/9">
