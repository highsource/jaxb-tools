    <xsd:simpleType name="ElectricityUnitsContentType">
        <xsd:annotation>
            <xsd:documentation>
                This code list represents the System Internationale's unit of measure for electricity.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:normalizedString">
            <xsd:enumeration value="ampere">
                <xsd:annotation>
                    <xsd:documentation source="http://www.unc.edu/~rowlett/units/">Ampere(amps)</xsd:documentation>
                </xsd:annotation>
