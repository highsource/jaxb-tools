                    <xsd:documentation source="http://www.unc.edu/~rowlett/units/">Cold Cranking Ampere(CCA)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="volt">
                <xsd:annotation>
                    <xsd:documentation source="http://www.unc.edu/~rowlett/units/">Volt(V)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
