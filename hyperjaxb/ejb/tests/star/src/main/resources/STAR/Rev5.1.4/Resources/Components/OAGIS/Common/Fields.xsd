         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MaidenName" type="NameType" />
   <xsd:element name="MaterialRequested" type="TextType">
      <xsd:annotation>
         <xsd:documentation source="http://www.starstandards.org">
