<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <artifactId>jaxb-maven-plugin-sample-episode-b-maven</artifactId>
  <name>Maven JAXB Plugin Sample [episode-b|maven]</name>
  <packaging>jar</packaging>
  <parent>
    <groupId>org.jvnet.jaxb</groupId>
    <artifactId>jaxb-maven-plugin-sample-episode-maven</artifactId>
    <version>${project.version}</version>
  </parent>
  <dependencies>
    <dependency>
      <groupId>org.glassfish.jaxb</groupId>
      <artifactId>jaxb-runtime</artifactId>
    </dependency>
    <dependency>
      <groupId>org.jvnet.jaxb</groupId>
      <artifactId>jaxb-maven-plugin-sample-episode-a-maven</artifactId>
      <version>${project.version}</version>
      <scope>provided</scope>
    </dependency>
  </dependencies>
  <build>
    <defaultGoal>test</defaultGoal>
    <plugins>
      <plugin>
        <groupId>org.jvnet.jaxb</groupId>
        <artifactId>jaxb-maven-plugin</artifactId>
        <configuration>
          <extension>true</extension>
          <episodes>
            <episode>
              <groupId>org.jvnet.jaxb</groupId>
              <artifactId>jaxb-maven-plugin-sample-episode-a-maven</artifactId>
            </episode>
          </episodes>
          <catalog>src/main/resources/catalog.cat</catalog>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
