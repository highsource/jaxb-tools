            <hj:id/>
        </jaxb:bindings>
        <jaxb:bindings node="xs:complexType[@name='ExtensibleObjectType']//xs:element[@ref='tns:Slot']">
		<hj:one-to-many>
			<orm:join-column name="SLOT_ID"/>
		</hj:one-to-many>
	</jaxb:bindings>
