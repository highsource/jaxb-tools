	</element>
	<element name="key" type="class">
		<attribute name="column" property="ColumnName"/>
	</element>
	<element name="index" type="class">
		<attribute name="column" property="ColumnName"/>
	</element>
	<element name="collection-id" type="class">
		<attribute name="column" property="ColumnName"/>
	</element>
	<element name="any" type="class">
		<content>
			<element-ref name="meta" property="Meta"/>
			<element-ref name="meta-value" property="MetaValie"/>
			<element-ref name="column" property="FirstColumn"/>
			<element-ref name="column" property="Columns"/>
		</content>
	</element>
	<element name="many-to-any" type="class">
		<content>
			<element-ref name="meta-value" property="MetaValie"/>
			<element-ref name="column" property="FirstColumn"/>
			<element-ref name="column" property="Columns"/>
		</content>
	</element>
	<element name="index-many-to-any" type="class">
		<content>
			<element-ref name="column" property="FirstColumn"/>
			<element-ref name="column" property="Columns"/>
		</content>
	</element>
	<element name="tuplizer" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="definition" type="class">
		<attribute name="class" property="Clazz"/>
	</element>
	<element name="version" type="class">
		<attribute name="column" property="ColumnName"/>
	</element>
	<element name="joined-subclass" type="class">
