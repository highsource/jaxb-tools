   <xsd:simpleType name="DateTimeType">
      <xsd:annotation>
         <xsd:documentation xml:lang="en">
            <ccts:UniqueID>QDT000000</ccts:UniqueID>
            <ccts:CategoryCode>QDT</ccts:CategoryCode>
            <ccts:DictionaryEntryName>Month_ Date. Type</ccts:DictionaryEntryName>
            <ccts:VersionID>1.0</ccts:VersionID>
            <ccts:DefinitionText>
               A particular point in the progression of time together with the relevant
               supplementary information. Provides the XML Schema simpleType that allows either just
               the Date or the Date and Time to be provided in the elements that are based on this
               type and/or types that use this as their base.
            </ccts:DefinitionText>
            <ccts:RepresentationTermName>Date</ccts:RepresentationTermName>
            <ccts:QualifierTerm>Month</ccts:QualifierTerm>
         </xsd:documentation>
      </xsd:annotation>
      <xsd:union memberTypes="udt:DateType udt:TimeType udt:DateTimeType" />
   </xsd:simpleType>
