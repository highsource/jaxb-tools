                    <xsd:documentation>Liters Per Hour</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="revolutions per minute">
                <xsd:annotation>
                    <xsd:documentation>Revolutions Per Minute (rpm)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="foot per second">
                <xsd:annotation>
                    <xsd:documentation>foot per second (ft/s)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="meter per second">
                <xsd:annotation>
                    <xsd:documentation>meter per second (m/s)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="TimeUnitsContentType">
        <xsd:annotation>
            <xsd:documentation>
                This code list represents the System Internationale's unit of measure for time.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:normalizedString">
            <xsd:enumeration value="nanosecond">
                <xsd:annotation>
                    <xsd:documentation source="http://www.unc.edu/~rowlett/units/">Nanosecond</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="microsecond">
                <xsd:annotation>
                    <xsd:documentation source="http://www.unc.edu/~rowlett/units/">Microsecond</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="millisecond">
                <xsd:annotation>
                    <xsd:documentation source="http://www.unc.edu/~rowlett/units/">Millisecond</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="second">
                <xsd:annotation>
                    <xsd:documentation source="http://www.unc.edu/~rowlett/units/">Second</xsd:documentation>
                </xsd:annotation>
