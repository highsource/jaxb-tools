            <xsd:element name="powerSupply" type="ns:powerSupplyType" minOccurs="0" maxOccurs="unbounded"/>
            
            <!-- temporary storage of data gathered by perl scripts -->
                
            <xsd:element name="memorySummary" type="xsd:string" minOccurs="0"/>
            <xsd:element name="diskSummary" type="xsd:string" minOccurs="0"/>                                                
            
            <!-- temporary (hopefully) storage of data gathered and examined by perl scripts -->                                
                
            <xsd:element name="zfsConfigCheckResults" type="xsd:string" minOccurs="0"/>
            <xsd:element name="svmConfigCheckResults" type="xsd:string" minOccurs="0"/>
            <xsd:element name="vxvmConfigCheckResults" type="xsd:string" minOccurs="0"/>
            <xsd:element name="mirroredDiskCheckResults" type="xsd:string" minOccurs="0"/>                
            <xsd:element name="patchCheckResults" type="xsd:string" minOccurs="0"/>
            
            <!-- information gathered by dream command -->
                
            <xsd:element name="dreamDisabledResults" type="xsd:string" minOccurs="0"/>
            <xsd:element name="clearcase" minOccurs="0" type="ns:clearcaseType"/>
            
            <!-- other -->
                
            <xsd:element name="javaTimeZoneVersion" type="xsd:string" minOccurs="0"/>                
            <xsd:element name="ufsDelq" type="ns:ufsDelqType" minOccurs="0"/>
            
        </xsd:sequence>
    </xsd:complexType>              
    
