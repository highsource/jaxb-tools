<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <artifactId>jaxb-plugins-tests</artifactId>
  <packaging>pom</packaging>
  <name>JAXB Tools :: JAXB Plugins :: Tests</name>
  <parent>
    <groupId>org.jvnet.jaxb</groupId>
    <artifactId>jaxb-plugins-project</artifactId>
    <version>3.0.2-SNAPSHOT</version>
  </parent>
  <properties>
    <maven.deploy.skip>true</maven.deploy.skip>
  </properties>
  <modules>
    <module>commons_lang</module>
    <module>defaultvalue</module>
    <module>episodes</module>
    <module>ignoring</module>
    <module>issues</module>
    <module>issues-legacy-ns</module>
    <module>JAXB-1058</module>
    <module>namespace</module>
    <module>one</module>
    <module>po</module>
    <module>qa-simple</module>
    <module>qa-strategic</module>
    <module>simple-hashCode-equals-01</module>
    <module>simplify-01</module>
    <module>simplify-02</module>
    <module>superclass</module>
    <module>wildcard</module>
    <module>zj</module>
  </modules>
  <dependencies>
    <dependency>
      <groupId>org.glassfish.jaxb</groupId>
      <artifactId>jaxb-runtime</artifactId>
    </dependency>
    <dependency>
      <groupId>org.jvnet.jaxb</groupId>
      <artifactId>jaxb-plugins-runtime</artifactId>
    </dependency>
    <dependency>
      <groupId>com.sun.activation</groupId>
      <artifactId>jakarta.activation</artifactId>
    </dependency>
    <dependency>
      <groupId>org.jvnet.jaxb</groupId>
      <artifactId>jaxb-plugins-testing</artifactId>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <build>
    <defaultGoal>test</defaultGoal>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-javadoc-plugin</artifactId>
        <configuration>
          <skip>true</skip>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
