          <action-soccer-other id="ACT2" period-value="1" period-minute-elapsed="4" action-type="note" comment="A slow tempo start where the teams test each other."/>
          <action-soccer-foul id="ACT3" period-value="1" period-minute-elapsed="6" foul-result="freekick" fouled-player-idref="SWE1-9" fouling-player-idref="HU1-2" comment="Ljungberg goes down under pressure - freekick 25 yards out."/>
