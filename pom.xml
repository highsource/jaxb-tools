	<properties>
		<maven.version>2.0.9</maven.version>
		<jaxb20.version>2.0.5</jaxb20.version>
		<jaxb21.version>2.1.13</jaxb21.version>
		<jaxb22.version>2.2.11</jaxb22.version>
