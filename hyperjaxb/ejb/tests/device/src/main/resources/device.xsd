            <xsd:element name="patch" type="ns:patchType" minOccurs="0" maxOccurs="unbounded"/>
            
            <xsd:element name="fcalBus" type="ns:fcalBusType" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="pciBus" type="ns:pciBusType" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="alom" type="ns:alomType" minOccurs="0"/>
            <xsd:element name="networkInterface" type="ns:networkInterfaceType" minOccurs="0" maxOccurs="unbounded"/>
            
            <!-- serengeti elements -->
                
            <xsd:element name="systemController" type="ns:systemControllerType" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="ioBoard" type="ns:ioBoardType" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="repeaterBoard" type="ns:repeaterBoardType" minOccurs="0" maxOccurs="unbounded"/>
